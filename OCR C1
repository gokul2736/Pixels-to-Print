        "from paddleocr import PaddleOCR\n",
        "import re\n",
        "\n",
        "ocr = PaddleOCR(use_angle_cls=True, lang='en')\n",
        "\n",
        "image_path = list(uploaded.keys())[0]\n",
        "\n",
        "result = ocr.ocr(image_path, cls=True)\n",
        "\n",
        "all_text_lines = [line[1][0] for line in result[0]]\n",
        "\n",
        "full_text = \"\\n\".join(all_text_lines)\n",
        "print(\"Extracted Full Text:\\n\", full_text)\n",
        "\n",
        "mrp_pattern = r\"MRP\\s*[:\\-]?\\s*([\\₹\\$\\£]?\\s*\\d+(\\.\\d{1,2})?)\"\n",
        "\n",
        "date_pattern = r\"(?:\\d{2}[-/]\\d{2}[-/]\\d{4}|\\d{2}[-/]\\d{4})\"\n",
        "\n",
        "manufacturer_pattern = r\"(Manufactured\\sby|Manufacturer\\s:\\s)(.+)\"\n",
        "\n",
        "mrp_match = re.search(mrp_pattern, full_text, re.IGNORECASE)\n",
        "date_match = re.search(date_pattern, full_text, re.IGNORECASE)\n",
        "manufacturer_match = re.search(manufacturer_pattern, full_text, re.IGNORECASE)\n",
        "\n",
        "print(\"\\nExtracted Information:\")\n",
        "\n",
        "if mrp_match:\n",
        "    print(\"MRP:\", mrp_match.group(1))\n",
        "else:\n",
        "    print(\"MRP: Not found\")\n",
        "\n",
        "if date_match:\n",
        "    print(\"Date:\", date_match.group())\n",
        "else:\n",
        "    print(\"Date: Not found\")\n",
        "\n",
        "if manufacturer_match:\n",
        "    print(\"Manufacturer Details:\", manufacturer_match.group(2))\n",
